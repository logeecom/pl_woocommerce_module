{
  "name": "packlink/woocommerce",
  "description": "Packlink WooCommerce Integration",
  "version": "3.0.0",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "no-api": true,
      "url": "git@github.com:logeecom/pl_module_core.git"
    }
  ],
  "require": {
    "php": ">=5.5",
    "packlink/integration-core": "dev-feature/ui-redesign",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-zip": "*",
    "iio/libmergepdf": "3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Packlink\\WooCommerce\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Logeecom\\Tests\\Infrastructure\\": "vendor/packlink/integration-core/tests/Infrastructure",
      "Logeecom\\Tests\\BusinessLogic\\": "vendor/packlink/integration-core/tests/BusinessLogic",
      "Logeecom\\Tests\\Common\\": "vendor/packlink/integration-core/tests/Common",
      "Packlink\\WooCommerce\\Tests\\": "../tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35"
  },
  "config": {
    "platform": {
      "php": "5.5.9"
    }
  },
  "scripts": {
    "post-update-cmd": "./copy-resources",
    "post-install-cmd": "./copy-resources"
  }
}
